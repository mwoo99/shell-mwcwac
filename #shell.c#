#include <string.h>
#include <stdio.h>
#include <unistd.h>

int main() {
    char input[256];
    char *s = input;
    printf("What would you like to do? ");
    fgets(input, 256, stdin);
    s = strsep(&s, "\n");

    char *command[256];
    int i = 0;
    while (s) {
        char *p = strsep(&s, " ");
        command[i] = p;
        i++;
    }
    command[i] = 0;
    execvp(command[0], command);
}